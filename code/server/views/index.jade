extends layout

block content
  div(class='container-fixed')
    div(class="row")
      div(class='col-xs-12')
        a(href="#", id="listen", class="btn btn-block btn-lg btn-danger") Listen
    div(class="row")
      div(class='col-xs-6')
        a(href="#", id="sleep", class="btn btn-block btn-lg btn-success") Sleep
      div(class='col-xs-6')
        a(href="#", id="wakeUp", class="btn btn-block btn-lg btn-primary") Wake Up 
    div(class="row")
      div(class='col-xs-6')
        a(href="#", id="sayHi", class="btn btn-block btn-lg btn-success") Say Hi
      div(class='col-xs-6')
        a(href="#", id="sayBye", class="btn btn-block btn-lg btn-primary") Say Bye
    div(class="row")
      div(class='col-xs-6')
        a(href="#", id="sayYes", class="btn btn-block btn-lg btn-success") Say YES
      div(class='col-xs-6')
        a(href="#", id="idle", class="btn btn-block btn-lg btn-success") Say No

    div(class="row")
      div(class='col-xs-6')
        a(href="#", id="showDance", class="btn btn-block btn-lg btn-success") Show Dance
      div(class='col-xs-6')
        a(href="#", id="showExpression", class="btn btn-block btn-lg btn-primary") Show Expression
    div(class="row")
      div(class='col-xs-6')
        a(href="#", id="activateMusic", class="btn btn-block btn-lg btn-success") Activate Music
      div(class='col-xs-6')
        a(href="#", id="deactivateMusic", class="btn btn-block btn-lg btn-success") Deactivate Music
    div(class="row")
      div(class='col-xs-6')
        a(href="#", id="playMusic", class="btn btn-block btn-lg btn-success") Play Music
      div(class='col-xs-6')
        a(href="#", id="stopMusic", class="btn btn-block btn-lg btn-primary") Stop Music
    div(class='row')
      div(class='col-xs-6')
        a(href="#", id="blockFacebook", class="btn btn-block btn-lg btn-success") Block Facebook
      div(class='col-xs-6')
        a(href="#", id="blockTwitter", class="btn btn-block btn-lg btn-primary") Block Twitter
    div(class='row')
      div(class='col-xs-6')
        a(href="#", id="unblockFacebook", class="btn btn-block btn-lg btn-success") Unblock Facebook
      div(class='col-xs-6')
        a(href="#", id="unblockTwitter", class="btn btn-block btn-lg btn-primary") Unblock Twitter
    div(class='row')
      div(class='col-xs-6')
        a(href="#", id="lightsOn", class="btn btn-block btn-lg btn-success") Lights On
      div(class='col-xs-6')
        a(href="#", id="lightsOff", class="btn btn-block btn-lg btn-primary") Lights Off
    div(class="row")
      div(class='col-xs-6')
        a(href="#", id="takePicture", class="btn btn-block btn-lg btn-success") Take Picture
      
    div(class="row")
      div(class='col-xs-6')
        a(href="#", id="blinkLed", class="btn btn-block btn-lg btn-success") Blink Led
      div(class='col-xs-6')
        a(href="#", id="stopIdle", class="btn btn-block btn-lg btn-primary") I Love you
    div(class="row")
      div(class='col-xs-12')
        a(href="#", id="resetAll", class="btn btn-block btn-lg btn-danger") Reset All
    

block javascript
  script(src='/js/global.js')